  margin: 0 auto;
}
/* 轮播图样式 */

.carousel_div {
  position: relative;
  width: 100%;
  height: 414rpx;
}

.swiper {
  position: absolute;
  height: 100%;
  width: 100%;
}

.swiper image {
  height: 100%;
  width: 100%;
}
.loginImg{
  width: 100%;
  height: 400rpx;
}
.fw{
  /* margin-top:30rpx;  */
  margin-left:4%; 
  display: flex;
  justify-content: space-between;
  width: 92%;
  flex-wrap: nowrap;
  /* height: 30rpx; */
}
.fw>text{
  font-size: 12px;
  line-height: 12rpx
}
/* .fw>view{
  line-height: 30rpx;
  height: 1px;
  width:45%;
  background: black;
} */
.fw>view{
  line-height: 30rpx;
  height: 1px;
  width:100%;
  background: #EBECF2;
}
.container{
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin-top: 60rpx;
}
.container>view{
  width: 30%;
  margin-bottom:60rpx ;
}
.container>view>image{
  display: block;
  width: 70%;
  margin: 30rpx auto;
  height: 100rpx;
}
.container>view>text{
  display: inline-block;
  width: 100%;
  text-align: center;
  font-size: 14px;

}
