    <view class='container pt10'>
      <view class="order_list" wx:for='{{order}}' wx:key="{{index}}">
        <view class="bw order_item">
          <navigator url="{{'/pages/orderListDetail/orderListDetail?oId='+item.oId}}" class="db plr30">
            <view class="flex_sb lh90 bdb font15">
               <text>订单编号:{{item.oId}}</text>
              <text class='{{item.oState==3?"color2":"color3"}}'>{{item.oState==0?"未支付":item.oState==1?"已支付":item.oState==2?"执行中":"已完成"}}</text>
            </view>
            <view class="goods">
              <view class="flex_a ptb30 goods_item" >
                <view class="goods_bg flex_aj">
                  <image src='{{good.img}}' class="goods_img" mode="widthFix"></image>
                </view>
                <view class="ml20">
                  <view class="font15 fw">服务类型 ： {{item.oType}}</view>
                  <view class="mt20 color2 fw">￥
                    <text class="font17 mr10">1111</text></view>
                </view>
              </view>
            </view>
            <view class="flex_sb lh90 bdt">
              <text>共1件商品</text>
              <text>共计：<text class='color2'>￥11</text></text>
            </view>
          </navigator>
        </view>
      </view>
      <view class='tac lh80 col9' wx:if='{{order.length<=0}}'>暂无更多数据</view>
    </view>
<!-- 删除订单弹框 -->
<van-dialog id="van-dialog" />