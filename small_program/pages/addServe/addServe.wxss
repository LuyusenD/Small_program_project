/* pages/addServe/addServe.wxss */
.ioic{
  position:relative;
  display:-webkit-flex;
  display:flex;
  width:100%;
  height: 120rpx;
  padding:10px 15px;
  font-size:14px;
  line-height:25px;
  color:#333;
  background-color:#fff;
  box-sizing:border-box;
}
.ioic image{
  width: 80rpx;
  height: 80rpx;
  margin-left: 100rpx
}
.center{
  width: 13 0px;
  height: 120px;
  position: absolute;
  top:0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 14px;
  text-align: center;
}/* pages/adminAddCar/adminAddCar.wxss */
.title{
  text-align: left;
  margin: 40rpx;
  font-size: 14px;
}
.table{
  border: 1px solid #CCCCCC;
  width:740rpx;
  margin: 0 auto;
  font-size:12px; 
}
.tr{
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  height: 80rpx;
}
.td{
  width: 245rpx;
  line-height: 80rpx;
  text-align: center;
  border-right: 1px solid #CCCCCC;
  border-bottom: 1px solid #CCCCCC;
}
.td:first-child{
  width: 340rpx;
  text-align: center;
  border-right: 1px solid #CCCCCC;
  border-bottom: 1px solid #CCCCCC;
}
.td:nth-child(2){
  width: 150rpx;
}
.table>view:last-child{
  border-bottom: none
}